template-areas:\n    \". up .\"\n    \"left reset right\"\n    \". down .\";\n}\n\n#keypad button {\n  border: 1px solid #dddddd;\n  border-radius: 28%;\n  align-self: center;\n  width: 55px;\n  height: 55px;\n}\n\n#keypad button:active {\n  box-shadow: 0 0 5px #938900;\n  border: 1px solid #7a9300;\n  background-color: white;\n}\n\nbutton#reset {\n  width: 90%;\n  height: 90%;\n  border-radius: 50%;\n  margin-left: 5px;\n  grid-area: reset;\n}\n\nbutton#left {\n  grid-area: left;\n}\n\nbutton#up {\n  grid-area: up;\n  margin-bottom: 3px;\n}\n\nbutton#right {\n  grid-area: right;\n  margin-left: 5px;\n}\n\nbutton#down {\n  grid-area: down;\n  margin-top: 3px;\n}\n\ninput {\n  height: 1.9rem;\n  border-radius: 0;\n  margin: -1px;\n  border: 1px solid #dddddd;\n  background-color: white;\n  outline: none;\n}\n\ninput[type=\"email\"] {\n  width: 230px;\n  margin-right: 1rem;\n}\n\ninput:active {\n  box-shadow: 0 0 5px #938900;\n  border: 1px solid #7a9300;\n}\n"},"lineCount":177}},"error":null,"hash":"9d3bedc9f07af603ec199199e4daa52b","cacheData":{"env":{}}}